<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.elephant.elephant_bi.mapper.SqlInfoMapper">
    <insert id="insert" parameterType="sqlInfo">
        insert into sql_info (name,creator,createTime,comment,shareAnalysis,dataOriginId,updateTime,workbookId,params)
        values(#{name},#{creator},#{createTime},#{comment},#{shareAnalysis},#{dataOriginId},#{updateTime},#{workbookId},#{params,typeHandler=com.elephant.elephant_bi.framework.ObjectJsonHandler})
    </insert>

    <delete id="delete" parameterType="int">
        delete from sql_info
        where workbookId = #{workbookId}
    </delete>
</mapper>